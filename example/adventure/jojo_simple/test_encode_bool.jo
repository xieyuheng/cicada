drop : { (- A) }
drop = {
  (let x : A)
}

swap : { (- A) (- A) A A }
swap = {
  (let x : A)
  (let y : A)
  x y
}

id : { (- A) A }
id = {
  (let x : A)
  x
}

true : { (- A) (- A) A }
true = {
  (let x : A)
  (let y : A)
  x
}

false : { (- A) (- A) A }
false = {
  (let x : A)
  (let y : A)
  y
}

not : { (- { (- A) (- A) A }) (- A) (- A) A }
not = {
  (let a : { (- A) (- A) A })
  (let x : A)
  (let y : A)
  x y a exe
}

true_not : { (- A) (- A) A }
true_not = {
  { true } not
}

true_not_not : { (- A) (- A) A }
true_not_not = {
  { { true } not } not
}

