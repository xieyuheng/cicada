class unit_t {}
object unit {}

class list_t { given A : type }
class null_t { given A : type }
class cons_t {
  given A : type
  given car : A
  given cdr : list_t(A)
}

let null_unit = null_t(unit_t)
let cons_unit_list = {
  given unit_list : list_t(unit_t)
  return cons_t(unit_t, unit, unit_list)
}
